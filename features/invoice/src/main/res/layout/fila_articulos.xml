<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:backgroundTint="#00FFFFFF">

    <ImageView
        android:id="@+id/imgEliminarArticulo"
        android:layout_width="30dp"
        android:layout_height="0dp"
        android:layout_marginStart="100dp"
        android:layout_weight="1"
        android:src="@drawable/delete"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvInvoiceListaArticulosPrecio"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <TextView
        android:id="@+id/tvInvoiceListaArticulosLabPrecio"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:fontFamily="@font/opensans_variablefont_wdth_wght"
        android:text="@string/tvListaArticulosLabPrecio"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/tvlabCantidad"
        app:layout_constraintStart_toEndOf="@+id/tvInvoiceListaArticulosLabNombre"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvInvoiceListaArticulosPrecio"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:fontFamily="@font/opensans_variablefont_wdth_wght"
        android:paddingStart="6sp"
        android:text="@string/tvListaArticulosPrecio"
        app:layout_constraintEnd_toEndOf="@+id/tvInvoiceListaArticulosLabPrecio"
        app:layout_constraintStart_toStartOf="@+id/tvInvoiceListaArticulosLabPrecio"
        app:layout_constraintTop_toBottomOf="@+id/tvInvoiceListaArticulosLabPrecio" />

    <TextView
        android:id="@+id/tvInvoiceListaArticulosNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:fontFamily="@font/opensans_variablefont_wdth_wght"
        android:paddingStart="6dp"
        android:text="@string/tvListaArticulosNombre"
        app:layout_constraintEnd_toEndOf="@+id/tvInvoiceListaArticulosLabNombre"
        app:layout_constraintStart_toStartOf="@+id/tvInvoiceListaArticulosLabNombre"
        app:layout_constraintTop_toBottomOf="@+id/tvInvoiceListaArticulosLabNombre" />

    <TextView
        android:id="@+id/tvInvoiceListaArticulosLabNombre"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dimenSeparacionLista"
        android:fontFamily="@font/opensans_variablefont_wdth_wght"
        android:text="@string/tvListaArticulosLabNombre"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:src="@drawable/compra"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvlabCantidad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="100dp"
        android:fontFamily="@font/opensans_variablefont_wdth_wght"
        android:text="Cantidad:"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvCantidad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:fontFamily="@font/opensans_variablefont_wdth_wght"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/tvlabCantidad"
        app:layout_constraintStart_toStartOf="@+id/tvlabCantidad"
        app:layout_constraintTop_toBottomOf="@+id/tvlabCantidad" />

</androidx.constraintlayout.widget.ConstraintLayout>